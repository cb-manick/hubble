{
  "$schema": "./schema/specification.json",
  "version": "0.0.0.1",
  "id": "https://hubble.io/invoice/",
  "uri": "https://hubble.io/invoice/",
  "features": [
    {
      "id": "https://hubble.io/invoice/pdfgeneration",
      "description": "Generates invoice Pdf",
      "operationRefName"="operationRefName",
      "produces": {
        "invocationType": "REST",
        "outputSchemaURI": "https://hubble.io/invoice/pdfgeneration#invoiceGenerationResponse"
      },
      "error": {
        "produces": "REST",
        "errorSchemaURI": "https://hubble.io/invoice/pdfgeneration#invoiceErrorSchema"
      }
    }
  ],
  "endpoints": [
    {
      "name": "operationRefName",
      "operation": "https://huble.io/invoice.openspec#generatepdf",
      "type": "rest"
    }
  ],
  "callBackEndpoints": [
    {
      "name": "generatedPdf",
      "operation": "https://huble.io/invoice.openspec#updatepdf",
      "type": "rest"
    }
  ],
  "updateErrorEndpoints": [
    {
      "name": "generatePdfError",
      "operation": "\"https://huble.io/invoice.openspec#updatepdf",
      "type": "rest"
    }
  ],
  "connectConfig": {
    "auth": "oauth",
    "headerName": "auth-header"
  },
  "connectBackConfig": {
    "auth": "apikey",
    "headerName": "auth-header"
  },
  "headers": [
    {
      "key": "x-provide",
      "value": "value"
    }
  ],
  "instrumentation": {
    "traceId": "xxx-sdfd-sdfag1"
  },
  "environment": {
    "live": {
      "host": "https://www.allinvoices.com/"
    },
    "test": {
      "host": "https://www.allinvoices-test.com/"
    },
    "integration": {
      "host": "https://www.allinvoices-int.com/"
    }
  },
  "scenario": {
    "id": "scenario1"
  }
}